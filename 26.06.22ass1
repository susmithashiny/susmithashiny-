#include <stdio.h>
#include<string.h>

int main() {
    char s[20];
    int i,c=0,x=0,k=0;
    printf("enter a string\n");
    gets(s);
    for(i=0;s[i]!='\0';i++)
    {
        if(isupper(s[i]))
        c++;
        else if(islower(s[i]))
        x++;
        else if(isdigit(s[i]))
        k++;
    }
    printf("upper case:%d\n",c);
    printf("lower case:%d\n",x);
    printf("numbers:%d\n",k);
}
output;
